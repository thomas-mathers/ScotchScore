{
  "name": "scotch-score",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "fix": "npm run lint:fix && npm run format:fix",
    "format:base": "prettier --cache --log-level warn",
    "format:fix": "npm run format:base -- --write \"src/**/*.{ts,tsx,html}\"",
    "format": "npm run format:base -- --check \"src/**/*.{ts,tsx,html}\"",
    "lint:base": "eslint --cache --ignore-path .eslintignore --ext ts,tsx",
    "lint:errors": "npm run lint:base -- --quiet .",
    "lint:fix": "npm run lint:base -- --fix .",
    "lint": "npm run lint:base .",
    "preview": "vite preview",
    "test:all": "npm run test:types && npm run test",
    "test:base": "vitest",
    "test:types": "tsc --noEmit",
    "test:watch": "npm run test:base -- watch",
    "test": "npm run test:base -- run"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "@mui/x-data-grid": "^6.19.5",
    "@tanstack/react-query": "^5.22.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-image-gallery": "^1.3.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router-dom": "^6.22.1",
    "react-timeago": "^7.2.0",
    "react-use": "^17.5.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react-dom": "^18.2.19",
    "@types/react-image-gallery": "^1.2.4",
    "@types/react-timeago": "^4.1.7",
    "@types/react": "^18.2.56",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint": "^8.56.0",
    "jsdom": "^24.0.0",
    "msw": "^2.2.1",
    "prettier": "3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  }
}
